{% extends '::ojsbase.html.twig' %}
{% block title %}{{ 'doi.config.title'|trans }} {{ parent() }}{% endblock %}

{% block breadcrumb %}
    {% set list = [
    {'link': path('ojs_journal_dashboard_index', {journalId: selectedJournal().id}), 'title': 'dashboard'|trans},
    {'title': 'doi.config.title'|trans}
    ] %}
    {{ breadcrumb(list) }}
{% endblock %}

{% block body -%}
    {% include '::flashbag.html.twig' %}

    <h1>{{ 'doi.config.title'|trans }}</h1>
    {{ form(form) }}
    <hr/>

    {{ getLogs(entity) }}

{% endblock %}
